<!doctype html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <a href="/new">Add new message</a>
    <div>
      <% messages.forEach((message) => { %>
        <h3><%= message.user %></h3>
        <p><%= message.text %></p>
      <% }) %>
    </div>
  </body>
</html>
